{
  "name": "florian-lacrampe-resume",
  "version": "1.0.0",
  "description": "Florian Lacrampe - Resume",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "build": "NODE_ENV=production gulp",
    "public": "rimraf dist && npm run build && node public.js",
    "serve": "npm run build && serve dist/",
    "watch": "npm-watch serve"
  },
  "watch": {
    "serve": {
      "patterns": [
        "template",
        "src",
        "data.yaml"
      ],
      "extensions": "ts,html,scss,yaml,css,js,styl",
      "quiet": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/xiao555/Resume-it.git"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.1.1",
    "npm-watch": "^0.11.0",
    "serve": "^13.0.2",
    "velocity-animate": "^1.4.3",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "browser-sync": "^2.18.7",
    "colors": "^1.1.2",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.1",
    "gray-matter": "^2.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-data": "^1.2.1",
    "gulp-fontmin": "^0.7.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nunjucks": "^3.0.0",
    "gulp-size": "^2.1.0",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "js-yaml": "^3.8.1",
    "markdown-it": "^8.3.0",
    "markdown-it-attrs": "^0.8.0",
    "nib": "^1.1.2",
    "path": "^0.12.7",
    "rimraf": "^2.6.1",
    "simple-git": "^1.73.0"
  },
  "author": "quadeare",
  "license": "MIT"
}
